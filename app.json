{
  "name": "coderdojochi",
  "scripts": {},
  "env": {
    "CONTACT_EMAIL": {
      "required": true
    },
    "DEBUG": {
      "required": true,
      "value": "True"
    },
    "DISABLE_COLLECTSTATIC": {
      "required": true,
      "value": "True"
    },
    "DEFAULT_FROM_EMAIL": {
      "required": true
    },
    "DJANGO_SECURE_SSL_REDIRECT": {
      "required": true
    },
    "ENABLE_DEV_FIXTURES": {
      "required": true
    },
    "STRIPE_LIVE_PUBLIC_KEY": {
      "required": true
    },
    "STRIPE_LIVE_SECRET_KEY": {
      "required": true
    },
    "STRIPE_LIVE_MODE": {
      "required": true
    },
    "DJSTRIPE_WEBHOOK_SECRET": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "SENDGRID_API_KEY": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql",
    "logdna"
  ],
  "buildpacks": [{
    "url": "heroku/python"
  }]
}
